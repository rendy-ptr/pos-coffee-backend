{
  "name": "pos-coffee-shop-backend",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "lint:check": "eslint \"src/**/*.{js,ts}\"",
    "lint:fix": "eslint \"src/**/*.{js,ts}\" --fix",
    "format:check": "prettier --check \"src/**/*.{js,ts}\"",
    "format:fix": "prettier --write \"src/**/*.{js,ts}\"",
    "prisma:generate": "npx prisma generate",
    "prisma:migrate": "npx prisma migrate dev",
    "prisma:fresh": "npx prisma db push --force-reset && npm run seed:all",
    "prisma:studio": "npx prisma studio",
    "prisma:validate": "prisma validate",
    "prisma:format": "prisma format",
    "prisma:reset": "npx prisma migrate reset",
    "prisma:push": "npx prisma db push",
    "seed:admin": "tsx src/seed/admin/admin.seed.ts",
    "seed:category": "tsx src/seed/admin/category.seed.ts",
    "seed:menu": "tsx src/seed/admin/menu.seed.ts",
    "seed:kasir": "tsx src/seed/admin/kasir.seed.ts",
    "seed:table": "tsx src/seed/admin/table.seed.ts",
    "seed:voucher": "tsx src/seed/voucherSeeder.ts",
    "seed:all": "npm run seed:admin && npm run seed:category && npm run seed:menu && npm run seed:kasir && npm run seed:table",
    "test": "vitest",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:coverage": "vitest run --coverage",
    "pre-commit": "lint-staged",
    "commit": "oco"
  },
  "lint-staged": {
    "**/*.{js,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "**/*.json": [
      "prettier --write"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^6.12.0",
    "@types/express": "^5.0.2",
    "bcrypt": "^6.0.0",
    "cloudinary": "^2.7.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dayjs": "^1.11.13",
    "express": "^5.1.0",
    "joi": "^18.0.2",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.6",
    "pino": "^9.7.0",
    "pino-http": "^10.4.0",
    "pino-pretty": "^13.0.0",
    "streamifier": "^0.1.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "zod": "^4.0.5"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.18",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/module-alias": "^2.0.4",
    "@types/multer": "^2.0.0",
    "@types/node": "^22.15.21",
    "@types/nodemailer": "^7.0.1",
    "@types/pino-http": "^5.8.4",
    "@types/streamifier": "^0.1.2",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "@typescript-eslint/utils": "^8.32.1",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/ui": "^4.0.18",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.4.0",
    "lint-staged": "^16.0.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.5.3",
    "prisma": "^6.12.0",
    "supertest": "^7.2.2",
    "ts-node": "^10.9.2",
    "tsc-watch": "^7.1.1",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "vitest": "^4.0.18"
  }
}
