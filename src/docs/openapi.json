{
  "openapi": "3.0.3",
  "info": {
    "title": "API Dokumentasi",
    "version": "1.0.0",
    "description": "Dokumentasi API untuk aplikasi Express TypeScript"
  },
  "servers": [
    {
      "url": "http://localhost:3000",
      "description": "Development server"
    }
  ],
  "paths": {
    "/users": {
      "get": {
        "summary": "Mendapatkan daftar pengguna",
        "tags": ["Users"],
        "responses": {
          "200": {
            "description": "Daftar pengguna berhasil diambil",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "id": { "type": "integer" },
                      "name": { "type": "string" }
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/test": {
      "get": {
        "summary": "Test endpoint",
        "tags": ["Test"],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "message": { "type": "string" }
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}
